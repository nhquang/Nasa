<Application x:Class="Nasa.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Nasa"
             xmlns:viewmodels="clr-namespace:Nasa.ViewModels"
             xmlns:Views="clr-namespace:Nasa.Views"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        
        <DataTemplate DataType="{x:Type viewmodels:CuriosityViewModel}">
            <Views:Curiosity/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:OpporunityViewModel}">
            <Views:Opportunity/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:SpiritViewModel}">
            <Views:Spirit/>
        </DataTemplate>
        
        
    </Application.Resources>
</Application>
